// << have higher precedence than |.
print(2 | 1 << 1) // expect: 2
print(1 << 1 | 2) // expect: 2

// << has higher precedence than &.
print(2 & 1 << 1) // expect: 2
print(1 << 1 & 2) // expect: 2

// << has higher precedence than ^.
print(2 ^ 1 << 1) // expect: 0
print(1 << 1 ^ 2) // expect: 0

// & has higher precedence than |.
print(1 & 1 | 2) // expect: 3
print(2 | 1 & 1) // expect: 3

// & has higher precedence than ^.
print(1 & 1 ^ 2) // expect: 3
print(2 ^ 1 & 1) // expect: 3

// ^ has higher precedence than |.
print(1 ^ 1 | 1) // expect: 1
print(1 | 1 ^ 1) // expect: 1
